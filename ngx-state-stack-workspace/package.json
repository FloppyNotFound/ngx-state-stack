{
  "name": "ngx-state-stack-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://FloppyNotFound/ngx-state-stack.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn pretty && ng build ngx-state-stack",

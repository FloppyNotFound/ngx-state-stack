    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.3.0",
    "prettier": "~1.18.2",
    "pretty-quick": "~1.11.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.3.0",
    "tsickle": "~0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "tslint-consistent-codestyle": "1.15.1",
    "typescript": "~3.7.0"
